{"ast":null,"code":"// @ is an alias to /src\n//1 插入1部分\n// const handleEdit = () => {\n//   console.log('click')\n// }\nimport request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      dialogTableVisible: false\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    handleClose() {},\n\n    load() {\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      request.post(\"/user\", this.form).then(res => {\n        console.log(res);\n      });\n    },\n\n    // 修改2部分\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      console.log(this.form);\n      this.dialogVisible = true;\n    },\n\n    handleDelete() {},\n\n    handleSizeChange() {},\n\n    handleCurrentChange() {},\n\n    currentPageChange() {}\n\n  }\n};","map":{"version":3,"mappings":"AAsGA;AACA;AACA;AACA;AACA;AAEA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELC,aAAa,EAAE,KAFV;MAGLC,MAAM,EAAE,EAHH;MAILC,WAAW,EAAE,CAJR;MAKLC,QAAQ,EAAE,EALL;MAMLC,KAAK,EAAE,CANF;MAOLC,SAAS,EAAE,EAPN;MAQLC,kBAAkB,EAAE;IARf,CAAP;EAUD,CAdY;;EAgBbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAlBY;;EAmBbC,OAAO,EAAE;IACPC,WAAW,GAAG,CAEb,CAHM;;IAIPF,IAAI,GAAG;MACLb,OAAO,CAACgB,GAAR,CAAY,OAAZ,EAAqB;QACnBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKX,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNF,MAAM,EAAE,KAAKA;QAHP;MADW,CAArB,EAMGa,IANH,CAMQC,GAAE,IAAK;QACbC,OAAO,CAACC,G