able-column prop=\"nickName\" label=\"昵称\"></el-table-column>\n      <el-table-column prop=\"sex\" label=\"性别\"></el-table-column>\n      <el-table-column prop=\"address\" label=\"地址\"></el-table-column>\n\n      <el-table-column fixed=\"right\" label=\"操作\">\n        <template slot-scope=\"scope\">\n            <el-link target=\"_blank\" @click=\"handleEdit(scope.row)\" >编辑</el-link>\n            <el-popconfirm\n                confirm-button-text='好的'\n                cancel-button-text='不用了'\n                icon=\"el-icon-info\"\n                icon-color=\"red\"\n                title=\"这是一段内容确定删除吗？\"\n            >\n              <el-link target=\"_blank\">删除</el-link>\n            </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin: 10px\">\n      <div class=\"demonstration\">All combined</div>\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :currentPage=\"currentPage\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\"\n      />\n      <el-dialog v-model=\"dialogTableVisible\" title=\"Shipping address\">\n        <el-table :data=\"gridData\">\n          <el-table-column property=\"date\" label=\"Date\" width=\"150\"/>\n          <el-table-column property=\"name\" label=\"Name\" width=\"200\"/>\n          <el-table-column property=\"address\" label=\"Address\"/>\n        </el-table>\n      </el-dialog>\n\n      <!-- Form -->\n      <el-button text @click=\"()=>{this.dialogFormVisible = true}\">open a Form nested Dialog</el-button>\n\n      <el-dialog\n          v-model=\"dialogVisible\"\n          title=\"提示\"\n          width=\"30%\"\n          :before-close=\"handleClose\"\n      >\n        <el-form :model=\"form\" label-width=\"120px\">\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"form.username\"/>\n          </el-form-item>\n          <el-form-item label=\"昵称\">\n            <el-input v-model=\"form.nickName\"/>\n          </el-form-item>\n          <el-form-item label=\"年龄\">\n            <el-input v-model=\"form.age\"/>\n          </el-form-item>\n          <el-form-item label=\"性别\">\n            <el-radio-group v-model=\"form.sex\">\n              <el-radio label=\"1\" size=\"large\">男</el-radio>\n              <el-radio label=\"2\" size=\"large\">女</el-radio>\n            </el-radio-group>\n          </el-form-item>\n          <el-form-item label=\"地址\">\n            <el-input v-model=\"form.address\"/>\n          </el-form-item>\n        </el-form>\n        <template>\n          <div>\n            <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确认</el-button\n            >\n          </span>\n          </div>\n        </template>\n      </el-dialog>\n    </div>\n    <!-- 为什么我的按钮出不来-->\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n//1 插入1部分\n// const handleEdit = () => {\n//   console.log('click')\n// }\n\nimport request from \"@/utils/request\";\n\nexport default {\n  name: 'HomeView',\n  components: {},\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      dialogTableVisible: false,\n    }\n  },\n\n  created() {\n    this.load()\n  },\n  methods: {\n    handleClose() {\n\n    },\n    load() {\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res)\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n    },\n    add() {\n      this.dialogVisible = true\n      this.form = {}\n    },\n    save() {\n      request.post(\"/user\", this.form).then(res => {\n        console.log(res)\n      })\n    },\n    // 修改2部分\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row))\n      console.log(this.form)\n      this.dialogVisible = true\n    },\n    handleDelete() {\n    },\n    handleSizeChange() {\n    },\n    handleCurrentChange() {\n    },\n    currentPageChange() {\n    }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}