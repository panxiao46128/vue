{"ast":null,"code":"// @ is an alias to /src\n//1 插入1部分\n// const handleEdit = () => {\n//   console.log('click')\n// }\nimport request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      dialogTableVisible: false,\n      bookList: [],\n      bookVis: false\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    showBooks(books) {\n      this.bookList = books;\n      this.bookVis = true;\n    },\n\n    handleClose() {},\n\n    load() {\n      request.get(\"/communication\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res); //将后台数据传给tableData\n\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"/communication\", this.form).then(res => {\n          console.log(res);\n          this.load(); //刷新\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else {\n        //新增\n        request.post(\"/communication\", this.form).then(res => {\n          console.log(res);\n        });\n      }\n    },\n\n    // 修改2部分\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/communication/\" + id);\n      this.load();\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变页数触发\n      this.currentPage = pageNum;\n      this.load();\n    },\n\n    currentPageChange() {}\n\n  }\n};","map":{"version":3,"mappings":"AA4GA;AACA;AACA;AACA;AACA;AAEA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELC,aAAa,EAAE,KAFV;MAGLC,MAAM,EAAE,EAHH;MAILC,WAAW,EAAE,CAJR;MAKLC,QAAQ,EAAE,EALL;MAMLC,KAAK,EAAE,CANF;MAOLC,SAAS,EAAE,EAPN;MAQLC,kBAAkB,EAAE,KARf;MASLC,QAAQ,EAAC,EATJ;MAULC,OAAO,EAAC;IAVH,CAAP;EAYD,CAhBY;;EAkBbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CApBY;;EAqBbC,OAAO,EAAE;IACPC,SAAS,CAACC,KAAD,EAAO;MACd,KAAKN,QAAL,GAAcM,KAAd;MACA,KAAKL,OAAL,GAAc,IAAd;IACD,CAJM;;IAKPM,WAAW,GAAG,CAEb,CAPM;;IAQPJ,IAAI,GAAG;MACLf,OAAO,CAACoB,GAAR,CAAY,gBAAZ,EAA8B;QAC5BC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKf,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNF,MAAM,EAAE,KAAKA;QAHP;MADoB,CAA9B,EAMGiB,IANH,CAMQC,GAAE,IAAK;QACbC,OAAO,CAACC,GAAR,CAAYF,GAAZ,EADa,CAEb;;QACA,KAAKd,SAAL,GAAiBc,GAAG,CAACrB,IAAJ,CAASwB,OAA1B;QAEA,KAAKlB,KAAL,GAAae,GAAG,CAACrB,IAAJ,CAASM,KAAtB;MACD,CAZD;IAaD,CAtBM;;IAuBPmB,GAAG,GAAG;MACJ,KAAKvB,aAAL,GAAqB,IAArB;MACA,KAAKD,IAAL,GAAY,EAAZ;IACD,CA1BM;;IA2BPyB,IAAI,GAAG;MACL,IAAG,KAAKzB,IAAL,CAAU0B,EAAb,EAAgB;QAAE;QAChB9B,OAAO,CAAC+B,GAAR,CAAY,gBAAZ,EAA8B,KAAK3B,IAAnC,EAAyCmB,IAAzC,CAA8CC,GAAE,IAAK;UACnDC,OAAO,CAACC,GAAR,CAAYF,GAAZ;UACD,KAAKT,IAAL,GAFoD,CAExC;;UACX,KAAKV,aAAL,GAAmB,KAAnB,CAHmD,CAG1B;QAC1B,CAJD;MAMF,CAPA,MAOM;QAAG;QACPL,OAAO,CAACgC,IAAR,CAAa,gBAAb,EAA+B,KAAK5B,IAApC,EAA0CmB,IAA1C,CAA+CC,GAAE,IAAK;UACpDC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QAED,CAHD;MAIF;IAID,CA5CM;;IA6CP;IACAS,UAAU,CAACC,GAAD,EAAM;MACd,KAAK9B,IAAL,GAAY+B,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeH,GAAf,CAAX,CAAZ;MACA,KAAK7B,aAAL,GAAqB,IAArB;IACD,CAjDM;;IAkDPiC,YAAY,CAACR,EAAD,EAAK;MACfL,OAAO,CAACC,GAAR,CAAYI,EAAZ;MACA9B,OAAO,CAACuC,MAAR,CAAe,oBAAoBT,EAAnC;MACA,KAAKf,IAAL;IAED,CAvDM;;IAwDPyB,gBAAgB,CAAChC,QAAD,EAAW;MAAE;MAC3B,KAAKA,QAAL,GAAcA,QAAd;MACA,KAAKO,IAAL;IACC,CA3DI;;IA4DP0B,mBAAmB,CAACnB,OAAD,EAAU;MAAE;MAC7B,KAAKf,WAAL,GAAiBe,OAAjB;MACA,KAAKP,IAAL;IACD,CA/DM;;IAgEP2B,iBAAiB,GAAG,CACpB;;EAjEO;AArBI,CAAf","names":["request","name","components","data","form","dialogVisible","search","currentPage","pageSize","total","tableData","dialogTableVisible","bookList","bookVis","created","load","methods","showBooks","books","handleClose","get","params","pageNum","then","res","console","log","records","add","save","id","put","post","handleEdit","row","JSON","parse","stringify","handleDelete","delete","handleSizeChange","handleCurrentChange","currentPageChange"],"sourceRoot":"","sources":["C:\\Users\\46128\\Desktop\\springboot框架vue\\vue\\src\\views\\Communication.vue"],"sourcesContent":["<template >\n  <div style=\"padding: 10px\">\n    <!--    </功能区域>-->\n\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-button type=\"primary\">导入</el-button>\n      <el-button type=\"primary\">导出</el-button>\n    </div>\n    <!--    搜索区域-->\n    <div style=\"margin: 10px 0\">\n\n      <el-input v-model=\"search\" placeholder=\"Please input\" style=\"width: 30%\" clearable/>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\n    </div>\n\n    <el-table :data=\"tableData\" style=\"width: 100%\">\n      <el-table-column prop=\"id\" label=\"ID\" sortable></el-table-column>\n      <el-table-column prop=\"context\" label=\"内容\"></el-table-column>\n      <el-table-column prop=\"address\" label=\"地址\"></el-table-column>\n\n\n        <el-table-column fixed=\"right\" label=\"操作\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" type=\"success\" target=\"_blank\" @click=\"showBooks(scope.row.bookList)\" >查看图书列表 </el-button>\n          <el-button size=\"mini\" target=\"_blank\" @click=\"handleEdit(scope.row)\" >编辑  </el-button>\n          <el-popconfirm title=\"Are you sure to delete this?\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button target=\"_blank\"  >删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin: 10px\">\n      <div class=\"demonstration\">All combined</div>\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :currentPage=\"currentPage\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\"\n      />\n\n      <el-dialog v-model=\"dialogTableVisible\" title=\"Shipping address\">\n        <el-table :data=\"gridData\">\n          <el-table-column property=\"date\" label=\"Date\" width=\"150\"/>\n          <el-table-column property=\"name\" label=\"Name\" width=\"200\"/>\n          <el-table-column property=\"address\" label=\"Address\"/>\n        </el-table>\n      </el-dialog>\n\n      <!-- Form -->\n      <el-button text @click=\"()=>{this.dialogFormVisible = true}\">open a Form nested Dialog</el-button>\n\n      <el-dialog title=\"用户拥有的图书列表\" v-model=\"bookVis\" width=\"30%\">\n       <el-table :data=\"bookList\" stripe border>\n        <el-table-column prop=\"id\" label=\"ID\"></el-table-column>\n         <el-table-column prop=\"name\" label=\"名称\"></el-table-column>\n         <el-table-column prop=\"price\" label=\"价格\"></el-table-column>\n       </el-table>\n      </el-dialog>\n      <el-dialog\n          v-model=\"dialogVisible\"\n          title=\"提示\"\n          width=\"30%\"\n          :before-close=\"handleClose\"\n      >\n\n        <el-form :model=\"form\" label-width=\"120px\">\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"form.username\"/>\n          </el-form-item>\n          <el-form-item label=\"昵称\">\n            <el-input v-model=\"form.nickName\"/>\n          </el-form-item>\n          <el-form-item label=\"年龄\">\n            <el-input v-model=\"form.age\"/>\n          </el-form-item>\n          <el-form-item label=\"性别\">\n            <el-radio-group v-model=\"form.sex\">\n              <el-radio label=\"1\" size=\"large\">男</el-radio>\n              <el-radio label=\"2\" size=\"large\">女</el-radio>\n            </el-radio-group>\n          </el-form-item>\n          <el-form-item label=\"地址\">\n            <el-input v-model=\"form.address\"/>\n          </el-form-item>\n\n        </el-form>\n        <template #footer>\n          <div>\n            <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取消</el-button>\n<!--              这里不能保存，要修改！！！你调用了保存接口！！！所以主键冲突了-->\n            <el-button type=\"primary\" @click=\"save\">确认</el-button>\n          </span>\n          </div>\n        </template>\n      </el-dialog>\n    </div>\n    <!-- 为什么我的按钮出不来-->\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n//1 插入1部分\n// const handleEdit = () => {\n//   console.log('click')\n// }\n\nimport request from \"@/utils/request\";\n\nexport default {\n  name: 'HomeView',\n  components: {},\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      dialogTableVisible: false,\n      bookList:[],\n      bookVis:false\n    }\n  },\n\n  created() {\n    this.load()\n  },\n  methods: {\n    showBooks(books){\n      this.bookList=books\n      this.bookVis =true\n    },\n    handleClose() {\n\n    },\n    load() {\n      request.get(\"/communication\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res)\n        //将后台数据传给tableData\n        this.tableData = res.data.records\n\n        this.total = res.data.total\n      })\n    },\n    add() {\n      this.dialogVisible = true\n      this.form = {}\n    },\n    save() {\n      if(this.form.id){ //更新\n        request.put(\"/communication\", this.form).then(res => {\n          console.log(res)\n         this.load() //刷新\n          this.dialogVisible=false //关闭弹窗\n        })\n\n      } else{  //新增\n        request.post(\"/communication\", this.form).then(res => {\n          console.log(res)\n\n        })\n      }\n\n\n\n    },\n    // 修改2部分\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n    },\n    handleDelete(id) {\n      console.log(id)\n      request.delete(\"/communication/\" + id)\n      this.load()\n\n    },\n    handleSizeChange(pageSize) { //改变当前个数触发\n      this.pageSize=pageSize\n      this.load()\n      },\n    handleCurrentChange(pageNum) { //改变页数触发\n      this.currentPage=pageNum\n      this.load()\n    },\n    currentPageChange() {\n    }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}