{"ast":null,"code":"// @ is an alias to /src\n//1 插入1部分\n// const handleEdit = () => {\n//   console.log('click')\n// }\nimport request from \"@/utils/request\";\nexport default {\n  name: 'Book',\n  components: {},\n\n  data() {\n    return {\n      user: {},\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      dialogTableVisible: false,\n      ids: []\n    };\n  },\n\n  created() {\n    let userStr = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(userStr);\n    this.load();\n  },\n\n  methods: {\n    handleSelectionChange(val) {\n      this.ids = val.map(v => v.id);\n    },\n\n    deleteBatch() {\n      if (!this.ids.length) {\n        this.$message.warning(\"请选择数据\");\n        return;\n      }\n\n      request.post(\"/book/deleteBatch\", this.ids).then(res => {\n        if (res.code === '0') {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.cover = res.data;\n    },\n\n    handleClose() {},\n\n    load() {\n      request.get(\"/book\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"/book\", this.form).then(res => {\n          console.log(res);\n          this.load(); //刷新\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else {\n        //新增\n        request.post(\"/book\", this.form).then(res => {\n          console.log(res);\n        });\n      }\n    },\n\n    // 修改2部分\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/book/\" + id);\n      this.load();\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变页数触发\n      this.currentPage = pageNum;\n      this.load();\n    },\n\n    currentPageChange() {}\n\n  }\n};","map":{"version":3,"mappings":"AAyHA;AACA;AACA;AACA;AACA;AAEA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELC,IAAI,EAAE,EAFD;MAGLC,aAAa,EAAE,KAHV;MAILC,MAAM,EAAE,EAJH;MAKLC,WAAW,EAAE,CALR;MAMLC,QAAQ,EAAE,EANL;MAOLC,KAAK,EAAE,CAPF;MAQLC,SAAS,EAAE,EARN;MASLC,kBAAkB,EAAE,KATf;MAULC,GAAG,EAAC;IAVC,CAAP;EAYD,CAhBY;;EAkBbC,OAAO,GAAG;IACR,IAAIC,OAAO,GAAEC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAgC,IAA7C;IACA,KAAKb,IAAL,GAAWc,IAAI,CAACC,KAAL,CAAWJ,OAAX,CAAX;IACA,KAAKK,IAAL;EACD,CAtBY;;EAuBbC,OAAO,EAAE;IACPC,qBAAqB,CAACC,GAAD,EAAK;MACxB,KAAKV,GAAL,GAASU,GAAG,CAACC,GAAJ,CAAQC,KAAKA,CAAC,CAACC,EAAf,CAAT;IACD,CAHM;;IAILC,WAAW,GAAE;MACb,IAAG,CAAC,KAAKd,GAAL,CAASe,MAAb,EAAoB;QAClB,KAAKC,QAAL,CAAcC,OAAd,CAAsB,OAAtB;QACA;MACF;;MACA9B,OAAO,CAAC+B,IAAR,CAAa,mBAAb,EAAkC,KAAKlB,GAAvC,EAA4CmB,IAA5C,CAAiDC,GAAE,IAAK;QACtD,IAAGA,GAAG,CAACC,IAAJ,KAAY,GAAf,EAAmB;UACd,KAAKL,QAAL,CAAcM,OAAd,CAAsB,QAAtB;UACH,KAAKf,IAAL;QACD,CAHD,MAGK;UACH,KAAKS,QAAL,CAAcO,KAAd,CAAoBH,GAAG,CAACI,GAAxB;QACF;MAED,CARD;IASD,CAlBM;;IAmBPC,kBAAkB,CAACL,GAAD,EAAK;MACrBM,OAAO,CAACC,GAAR,CAAYP,GAAZ;MACA,KAAK5B,IAAL,CAAUoC,KAAV,GAAgBR,GAAG,CAAC9B,IAApB;IACD,CAtBM;;IAuBPuC,WAAW,GAAG,CAEb,CAzBM;;IA0BPtB,IAAI,GAAG;MACLpB,OAAO,CAAC2C,GAAR,CAAY,OAAZ,EAAqB;QACnBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKrC,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNF,MAAM,EAAE,KAAKA;QAHP;MADW,CAArB,EAMGyB,IANH,CAMQC,GAAE,IAAK;QACbM,OAAO,CAACC,GAAR,CAAYP,GAAZ;