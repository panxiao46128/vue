{"ast":null,"code":"// @ is an alias to /src\nimport request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      request.post(\"/user\", this.form).then(res => {\n        console.log(res);\n      });\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete() {},\n\n    handleSizeChange() {},\n\n    handleCurrentChange() {},\n\n    currentPage() {}\n\n  }\n};","map":{"version":3,"mappings":"AAmGA;AAEA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE,EAFC;;EAKbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAC,EADA;MAELC,aAAa,EAAC,KAFT;MAGLC,MAAM,EAAC,EAHF;MAILC,WAAW,EAAE,CAJR;MAKLC,QAAQ,EAAE,EALL;MAMLC,KAAK,EAAE,CANF;MAOLC,SAAS,EAAC;IAPL,CAAP;EASD,CAfY;;EAiBbC,OAAM,GAAK;IACT,KAAKC,IAAL;EACD,CAnBY;;EAoBbC,OAAO,EAAE;IACPD,IAAI,GAAE;MACJZ,OAAO,CAACc,GAAR,CAAY,OAAZ,EAAqB;QACnBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKT,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNF,MAAM,EAAE,KAAKA;QAHP;MADW,CAArB,EAMGW,IANH,CAMQC,GAAE,IAAI;QACZC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKR,SAAL,GAAiBQ,GAAG,CAACf,IAAJ,CAASkB,OAA1B;QACD,KAAKZ,KAAL,GAAYS,GAAG,CAACf,IAAJ,CAASM,KAArB;MACA,CAVD;IAWD,CAbM;;IAcPa,GAAG,GAAE;MACH,KAAKjB,aAAL,GAAoB,IAApB;MACA,KAAKD,IAAL,GAAU,EAAV;IACD,CAjBM;;IAkBPmB,IAAI,GAAE;MACJvB,OAAO,CAACwB,IAAR,CAAa,OAAb,EAAqB,KAAKpB,IAA1B,EACAa,IADA,CACKC,GAAE,IAAI;QACTC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACD,CAHD;IAID,CAvBM;;IAwBPO,UAAU,CAACC,GAAD,EAAK;MACb,KAAKtB,IAAL,GAAUuB,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeH,GAAf,CAAX,CAAV;MACA,KAAKrB,aAAL,GAAmB,IAAnB;IACD,CA3BM;;IA4BPyB,YAAY,GAAE,CAAE,CA5BT;;IA6BPC,gBAAgB,GAAE,CAAE,CA7Bb;;IA8BPC,mBAAmB,GAAE,CAAE,CA9BhB;;IA+BPzB,WAAW,GAAE,CAAC;;EA/BP;AApBI,CAAf","names":["request","name","components","data","form","dialogVisible","search","currentPage","pageSize","total","tableData","created","load","methods","get","params","pageNum","then","res","console","log","records","add","save","post","handleEdit","row","JSON","parse","stringify","handleDelete","handleSizeChange","handleCurrentChange"],"sourceRoot":"","sources":["C:\\Users\\46128\\Desktop\\springboot框架vue\\vue\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n<!--    </功能区域>-->\n\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-button type=\"primary\">导入</el-button>\n      <el-button type=\"primary\">导出</el-button>\n    </div>\n<!--    搜索区域-->\n    <div style=\"margin: 10px 0\">\n\n      <el-input v-model=\"search\" placeholder=\"Please input\" style=\"width: 30%\" clearable/>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\" >查询</el-button>\n    </div>\n\n    <el-table :data=\"tableData\" style=\"width: 100%\">\n      <el-table-column prop=\"id\" label=\"ID\"  sortable></el-table-column>\n      <el-table-column prop=\"username\" label=\"用户名\"  ></el-table-column>\n      <el-table-column prop=\"nickName\" label=\"昵称\" ></el-table-column>\n      <el-table-column prop=\"sex\" label=\"性别\" ></el-table-column>\n      <el-table-column prop=\"address\" label=\"地址\" ></el-table-column>\n\n      <el-table-column fixed=\"right\" label=\"操作\" >\n        <template #default>\n          <el-button link type=\"primary\"  @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"Are you sure to delete this?\">  \n            <template #reference>\n              <el-button type=\"text\" @click=\"handleDelete(scope.$index,scope.row)\">删除</el-button>\n            </template>\n          </el-popconfirm>\n\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin: 10px\">\n      <div class=\"demonstration\">All combined</div>\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :currentPage=\"currentPage\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\"\n      />\n      <el-dialog v-model=\"dialogTableVisible\" title=\"Shipping address\">\n        <el-table :data=\"gridData\">\n          <el-table-column property=\"date\" label=\"Date\" width=\"150\" />\n          <el-table-column property=\"name\" label=\"Name\" width=\"200\" />\n          <el-table-column property=\"address\" label=\"Address\" />\n        </el-table>\n      </el-dialog>\n\n      <!-- Form -->\n      <el-button text @click=\"dialogFormVisible = true\"\n      >open a Form nested Dialog</el-button\n      >\n\n      <el-dialog\n          v-model=\"dialogVisible\"\n          title=\"提示\"\n          width=\"30%\"\n          :before-close=\"handleClose\"\n      >\n        <el-form :model=\"form\"  label-width=\"120px\">\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"form.username\" />\n          </el-form-item>\n          <el-form-item label=\"昵称\">\n            <el-input v-model=\"form.nickName\" />\n          </el-form-item>\n          <el-form-item label=\"年龄\">\n            <el-input v-model=\"form.age\" />\n          </el-form-item>\n          <el-form-item label=\"性别\" >\n           <el-radio-group v-model=\"form.sex\">\n             <el-radio label=\"1\" size=\"large\">男</el-radio>\n             <el-radio label=\"2\" size=\"large\">女</el-radio>\n           </el-radio-group>\n          </el-form-item>\n          <el-form-item label=\"地址\">\n            <el-input v-model=\"form.address\" />\n          </el-form-item>\n        </el-form>\n         <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确认</el-button\n        >\n      </span>\n        </template>\n      </el-dialog>\n    </div>\n<!-- 为什么我的按钮出不来-->\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\nimport request from \"@/utils/request\";\n\nexport default {\n  name: 'HomeView',\n  components: {\n\n  },\n  data(){\n    return {\n      form:{},\n      dialogVisible:false,\n      search:'',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData:[]\n    }\n  },\n\n  created () {\n    this.load()\n  },\n  methods: {\n    load(){\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res =>{\n        console.log(res)\n        this.tableData = res.data.records\n       this.total =res.data.total\n      })\n    },\n    add(){\n      this.dialogVisible =true\n      this.form={}\n    },\n    save(){\n      request.post(\"/user\",this.form).\n      then(res =>{\n        console.log(res)\n      })\n    },\n    handleEdit(row){\n      this.form=JSON.parse(JSON.stringify(row))\n      this.dialogVisible=true\n    },\n    handleDelete(){},\n    handleSizeChange(){},\n    handleCurrentChange(){},\n    currentPage(){}\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}